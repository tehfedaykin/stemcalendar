    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "moment": "^2.10.6",
    "ng-annotate-loader": "0.0.10",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "angular": "^1.4.8",
    "angular-ui-router": "^0.2.15",
    "babel": "^5.8.19",
